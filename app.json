{
  "expo": {
    "name": "찰나",
    "slug": "chalna",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chalna-app-icon1.png",
    "scheme": "chalna",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#000000",
      "bundleIdentifier": "com.anonymous.chalna"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chalna-app-icon1.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/chalna-app-icon1.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/chalna-app-icon1.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#000000",
          "dark": {
            "image": "./assets/images/chalna-app-icon1.png",
            "backgroundColor": "#000000"
          },
          "light": {
            "image": "./assets/images/chalna-app-icon1.png",
            "backgroundColor": "#0000FF"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
